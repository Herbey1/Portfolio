{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "cache-control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)\.html",
      "headers": [
        { "key": "cache-control", "value": "no-store" }
      ]
    }
  ]
}

